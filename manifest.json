{
    "name": "Spaces",
    "description": "Intuitive tab management",
    "version": "1.1.3",
    "permissions": [
        "tabs",
        "tabGroups",
        "storage",
        "history",
        "unlimitedStorage",
        "contextMenus"
    ],
    "--removed permissions--": [
        "chrome://favicon/*"
    ],
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "action": {
        "default_title": "Spaces",
        "default_icon": {
            "16": "img/icon16.png",
            "48": "img/icon48.png",
            "128": "img/icon128.png"
        },
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "spaces.html"
    },
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "--incognito--": "split",
    "manifest_version": 3,
    "minimum_chrome_version": "35",

    "commands": {
        "spaces-move": {
            "description": "Move active tab",
            "suggested_key": {
                "default": "Ctrl+Shift+M",
                "mac": "Command+Shift+M"
            }
        },
        "spaces-switch": {
            "description": "Switch between spaces",
            "suggested_key": {
                "default": "Ctrl+Shift+S",
                "mac": "Command+Shift+S"
            }
        }
    }
}
